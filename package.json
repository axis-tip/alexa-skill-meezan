{
  "name": "alexa-skill-meezan",
  "version": "0.0.1",
  "engines": {
    "node": "4.3.2",
    "npm": "3.5.0"
  },
  "description": "An Alexa skill for the Amazon Echo.",
  "license": "MIT",
  "main": "build/index.js",
  "repository": "tjaffri/alexa-skill-meezan",
  "scripts": {
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test tools",
    "build": "cross-env BABEL_ENV=production npm-run-all --parallel build:skill build:utterances",
    "build:skill": "webpack -p --config config/webpack.config.babel.js",
    "build:utterances": "mkdir -p build && bin/utterances --schema model/schema.json --utterances model/UTTERANCES --dictionary model/dictionary.json > build/UTTERANCES",
    "pretest": "npm run build",
    "test": "cross-env BABEL_ENV=test nyc ava --concurrency=2",
    "posttest": "npm run lint",
    "prepackage": "npm run test",
    "package": "bestzip build/package.zip build/index.js build/UTTERANCES"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-core/register"
    ]
  },
  "dependencies": {
    "alexa-annotations": "^6.0.0",
    "alexa-response": "^3.2.0",
    "alexa-ssml": "^0.5.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "alexa-utterances": "^0.1.0",
    "ava": "^0.16.0",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.7.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "bestzip": "^1.1.3",
    "coveralls": "^2.11.14",
    "cross-env": "^3.0.0",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "node-aws-lambda": "^0.1.8",
    "nomnom": "^1.8.1",
    "npm-run-all": "^1.7.0",
    "nyc": "^8.3.0",
    "webpack": "^1.12.14"
  },
  "files": [
    "build",
    "src"
  ],
  "keywords": [
    "alexa",
    "alexa-skill",
    "echo",
    "skill"
  ]
}
